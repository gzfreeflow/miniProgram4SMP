/*mask*/

.map {
  width: 330rpx;
}

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #fff;
  opacity: 0.5;
  overflow: hidden;
}

/*content*/

.drawer_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  /* bottom: 10px; */
  top: 0;
  left: 0;
  z-index: 1001;
  background: #fafafa;
  /* margin: -150px 50rpx 0 50rpx; */
  border-radius: 3px;
}